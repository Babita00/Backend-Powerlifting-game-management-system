{
  "name": "athleticore-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc -b && vite build",
    "dev": "nodemon -w src/server -x tsx src/server/main.ts",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "migration:generate": "tsx -r tsconfig-paths/register src/config/db.ts && tsx -r tsconfig-paths/register node_modules/typeorm/cli.js migration:generate src/server/migrations/migration -d src/server/database/database.ts",
    "migration:run": "tsx -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d src/server/database/database.ts",
    "migration:revert": "tsx -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -d src/server/database/database.ts",
    "migration:show": "tsx -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:show -d src/server/database/database.ts",
    "predev": "tsoa spec-and-routes",
    "prepare": "husky && npm run build",
    "prestart": "npm run build",
    "start": "NODE_ENV=production tsx src/server/main.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "helmet": "^8.1.0",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.28",
    "winston": "^3.19.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.8",
    "eslint": "^9.39.2",
    "prettier": "^3.7.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typeorm-ts-node-commonjs": "^0.3.20",
    "typescript": "^5.9.3"
  }
}
